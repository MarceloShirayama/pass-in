    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-istanbul": "^1.5.1",
    "supertest": "^7.0.0",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.1"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
